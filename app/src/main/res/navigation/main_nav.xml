<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/financial_nav"
    app:startDestination="@id/normalFragment">

    <fragment
        android:id="@+id/normalFragment"
        android:name="com.khairo.basessample.ui.normal.NormalFragment"
        android:label="NormalFragment"
        tools:layout="@layout/fragment_normal">

        <action
            android:id="@+id/action_normalFragment_to_adapterFragment"
            app:destination="@id/adapterFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>

    <fragment
        android:id="@+id/adapterFragment"
        android:name="com.khairo.basessample.ui.adapter.AdapterFragment"
        android:label="AdapterFragment"
        tools:layout="@layout/adapter_fragment" />
</navigation>
